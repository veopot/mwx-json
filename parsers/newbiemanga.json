	"manga_search_complete": {
		"headers": {
			"Accept": "*/*",
			"Referer": "https://newmanga.org/",

			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		"search_link": "https://api.newmanga.org/v2/projects/search?query=$query$&size=10&page=$page$",
		"query_type": "GET",


		"add_manga": {
			"start": "\"items\":[",
			"end": "],",
			"next": "\"title\":{",

			"link": {
				"before": "\"en\":\"",
				"token1": "id\":",
				"token2": ",",
				"replace": {
					"prefix": "https://api.newmanga.org/v2/projects/"
				}
			},


			"title": {
			"skip": "title\"",
			"token1": "\"en\":\"",
			"token2": "},",
			"replace": [ 
			{"match": "\",\"ru\":\"","text": " | "},
			{"match": "\",\"original\":null","text": ""},
			{"match": "\",\"original\":\".*?\"","text": ""}
			 ]
		},

			"uniq": {
				"skip": "title\"",
				"token1": "\"en\":\"",
				"token2": "\","
			},

			"cover": {
				"token1": "\"large\":\"images/projects/",
				"token2": "\",",
				"replace": [{
						"prefix": "https://storage.newmanga.org/images/projects/"
					},
					{
						"match": "u002F",
						"text": "/"
					}
				]
			},

			"summary": {
				"token1": "\"description\":\"",
				"token2": "\"",
				"replace": [{
						"match": "u003C",
						"text": ""
					},
					{
						"match": "<.*?>",
						"text": ""
					}
				]
			}

		}

	},



	"manga_list_complete": {
		"headers": {

			"accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9",
			"Accept-Language": "ru-RU,ru;q=0.9"
		},

		"iterator": {
			"base_url": "https://api.newmanga.org/v2/projects/catalog?query=&sorting=rating&size=32&page=",

			"append_nums": {"from": 1,"to": 57}
		},
		"add_manga": {
			"start": "\"items\":[",
			"end": "],",
			"next": "\"title\":{",

			"link": {
				"before": "\"en\":\"",
				"token1": "id\":",
				"token2": ",",
				"replace": {
					"prefix": "https://api.newmanga.org/v2/projects/"
				}
			},


			"title": {
			"skip": "title\"",
			"token1": "\"en\":\"",
			"token2": "},",
			"replace": [ 
			{"match": "\",\"ru\":\"","text": " | "},
			{"match": "\",\"original\":null","text": ""},
			{"match": "\",\"original\":\".*?\"","text": ""}
			 ]
		},


			"uniq": {
				"skip": "title\"",
				"token1": "\"en\":\"",
				"token2": "\","
			}

		},
		"rating_auto_inc": {
			"start": "20000",
			"inc": -1
		}

	},




	"manga_complete": {
		"headers": {
			"Referer": "https://newmanga.org/"
		},


		"link_corrector": [{
			"match": "https://newmanga.org/p/",
			"text": "https://api.newmanga.org/v2/projects/"
		}],


		"title": {
			"skip": "title\"",
			"token1": "\"en\":\"",
			"token2": "},",
			"replace": [ 
			{"match": "\",\"ru\":\"","text": " | "},
			{"match": "\",\"original\":null","text": ""},
			{"match": "\",\"original\":\".*?\"","text": ""}
			 ]
		},

		"uniq": {
			"skip": "title\"",
			"token1": "\"en\":\"",
			"token2": "\","
		},

		"additional_title": {
			"skip": "title\"",
			"token1": "\"en\":\"",
			"token2": "\","
		},

		"author": {
			"skip": "\"author\":{",
			"token1": "\"name\":\"",
			"token2": "\"",
			"replace": {
				"match": "<.*>",
				"text": " "
			}
		},


		"cover": {
			"token1": "\"large\":\"images/projects/",
			"token2": "\",",
			"replace": [{
					"prefix": "https://storage.newmanga.org/images/projects/"
				},
				{
					"match": "u002F",
					"text": "/"
				}
			]

		},

		"read_dir": {
			"right_to_left": "\"manga\"",
			"left_to_right": "\"manhya\""
		},

		"summary": {
			"skip": "\"type\":\"",
			"token1": "\"description\":\"",
			"token2": "\",",

			"replace": [{
					"match": "u003Cp",
					"text": ""
				},
				{
					"match": "u003E",
					"text": ""
				},
				{
					"match": "u003C",
					"text": ""
				},
				{
					"match": "<.*?>",
					"text": ""
				},
				{
					"match": "\n",
					"text": " "
				},
				{
					"match": "u002Fp",
					"text": ""
				}
			]
		},


		"status": {
			"complete": {
				"start": "\"status\":\" ",
				"end": "\",",
				"values": ["complete", "completed", "Сингл"]
			},
			"ongoing": {
				"start": "\"status\":\" ",
				"end": "\",",
				"value": "on_going"
			}
		},
		
		"rating": {
			"token1": "\"rating\":",
			"token2": ","
		},



		"add_tags": {
			"start": "genres\":[",
			"end": "],",
			"token1": "\"ru\":\"",
			"token2": "\""
		},




		"is_mature": {
			"values": ["\"adult\":\"18\",", "Эротика", "Юри", "Яой"]
		},



		"chapters_from_page": {
			"before": "\"type\":\"",
			"token1": "\"id\":",
			"token2": ",",
			"replace": [{
					"prefix": "https://api.newmanga.org/v2/projects/"
				},
				{
					"sufix": "/chapters?reverse=true&size=1000000"
				}
			]
		},
		"add_chapter": {
			"start": "items",
			"end": "]",
			"next": "\"is_available\":true",

			"link": {
				"before": "\"pages\":",
				"token1": "id\":",
				"token2": ",\"",
				
				"replace": [
			{"prefix": "https://api.newmanga.org/v2/chapters/"}, 
			{"sufix": "/pages"} 
			        ]
			},


			"uniq": {
				"before": "\"pages\":",
				"token1": "\"tom\":",
				"token2": ".0,\"hearts\"",
				"replace": [{
						"match": "\"name\":(.*?),",
						"text": " "
					},
					{
						"match": "\"number\":",
						"text": "Глава "
					},
					{
						"match": "\"name\":null,",
						"text": " "
					},
					{
						"match": ",",
						"text": " "
					},
					{
						"prefix": "Том "
					}
				]
			},

			"title": {
				"before": "\"pages\":",
				"token1": "\"tom\":",
				"token2": ".0,\"hearts\"",
				"replace": [{
						"match": "\"name\":(.*?),",
						"text": " "
					},
					{
						"match": "\"number\":",
						"text": "Глава "
					},
					{
						"match": "\"name\":null,",
						"text": " "
					},
					{
						"prefix": "Том "
					}
				]
			}
		}

	},

