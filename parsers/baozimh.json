{
    "id": 41135,
    "version": 6,
    "title": "包子漫畫",
    "host": "https://www.baozimh.com",
    "public_link": "https://www.baozimh.com/classify",
    "folder": "baozimh",
    "name": "baozimh",
    "chapters_order": true,
    "language": "cn",
    "encoding": "utf-8",
    
    "icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/baozimh.png",
    "self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/baozimh.json",
    "self_link2": "https://fant.my1.ru/mwparser/mwx/json/baozimh.json",
    "catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/baozimh.zip",
    "file_type": "sqlite",
    "author": {
        "name": "Veopot",
        "email": "fant-net@ya.ru"
    },
    
   
    "manga_list_complete": {
       
        "iterator": {
            "base_url": "https://www.baozimh.com/api/bzmhq/amp_comic_list?type=all&region=all&filter=*&page=",
            "append_nums": { 
			"from": 1,
			"to": 800,
			"step": 1,
			"sufix": "&limit=50&language=tw" }
			
        },
        "add_manga": {
            "next": "{\"comic",
			
            "link": {
                "token1": "_id\":\"",
                "token2": "\",",
                "replace":{ "prefix": "https://www.baozimh.com/comic/" }
            },
			
            
            "title": {
				"skip": "_id\":\"",
                "token1": "\"name\":\"",
                "token2": "\""
			},
            "uniq": {
				
               "token1": "id\":\"",
               "token2": "\","
            }
        },
        "rating_auto_inc": {
            "start": "50000",
            "inc": -1
        }
    },
	
	"manga_search_complete": {		
		"search_link": "https://www.baozimh.com/search?q=$query$",
		"query_type": "GET",
		"add_manga": {
			"next": "<div class=\"comics-card pure-u-1-2",
			
			"link":  { "token1": "href=\"", "token2": "\"",
				"replace": {
					"prefix": "https://www.baozimh.com"
				} },
				
			"title": { "token1": "<div class=\"comics-card__title\">", "token2": "</" },
			"uniq":  { "token1": " href=\"/comic/", "token2": "\"" },
			"cover": { "token1": "src=\"", "token2": "?w=" }
			
		}
	},



    "manga_complete": {
		 "headers": {
            "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
            "Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
            "Referer": "https://www.baozimh.com/",
            "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/141.0.0.0 Safari/537.36"
            },
        
        "title": {
			"after": "<h1 class=\"comics-detail__title\"",
            "token1": ">",
            "token2": "</h1>",
			
			"replace": [
                {
                    "match": "\"",
                    "text": "'"
                },
                {
                    "match": "\\\\",
                    "text": ""
                }
            ]
        },
		
        
        
		"uniq": {
			"after": "name=\"og:url\"",
            "token1": "/comic/",
            "token2": "\"",
			
            "replace": [
                 {
                        "match": "amp;",
                        "text": ""
                },
                {
                    "match": "u003E",
                    "text": ""
                },
                {
                    "match": "u003C",
                    "text": ""
                },
                {
                    "match": "<.*?>",
                    "text": ""
                },
                {
                    "match": "u002Fp",
                    "text": ""
                },
                {
                    "match": "&quot;",
                    "text": "'"
                }
            ]
        },
		
       
		"cover": {
			"after": "name=\"og:image\"",
            "token1": "content=\"",
            "token2": "\"",
			
            "replace": [
                 {
                        "match": "amp;",
                        "text": ""
                },
                {
                    "match": "u003E",
                    "text": ""
                },
                {
                    "match": "u003C",
                    "text": ""
                },
                {
                    "match": "<.*?>",
                    "text": ""
                },
                {
                    "match": "u002Fp",
                    "text": ""
                },
                {
                    "match": "&quot;",
                    "text": "'"
                }
            ]
        },
		
		"status": {

			"complete": {
				"values": ["已完结", "已完結", "COMPLETED"]
			},

			"ongoing": {
				"values": ["连载中", "連載中", "ONGOING"]
			}
		},

		"add_tag": { "after": "<span class=\"tag\"", "token1": ">", "token2": "</" },
		
		"author":      { "after": "<h2 class=\"comics-detail__author\"", "token1": ">", "token2": "</" },
		
		"summary": {
			"after": "<p class=\"comics-detail__desc",
            "token1": ">",
            "token2": "</p>",
			
            "replace": [
                {
                    "match": "u003Cp",
                    "text": ""
                },
                {
                    "match": "u003E",
                    "text": ""
                },
                {
                    "match": "u003C",
                    "text": ""
                },
                {
                    "match": "<.*?>",
                    "text": ""
                },
                {
                    "match": "u002Fp",
                    "text": ""
                },
                {
                    "match": "&quot;",
                    "text": "'"
                }
            ]
        },
       
       
       
        
        "add_chapter": {
			"headers": {
            "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
            "Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
            "Referer": "https://www.baozimh.com/",
            "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/141.0.0.0 Safari/537.36"
            },
            "start": "<div id=\"chapter-items\"",
            "end": "</button>",
            "next": "href=\"/user/page_direct",
			
            "link": {
                "after": "?comic_id",
                "token1": "=",
                "token2": "\"",
                "replace": [
                    {
                        "prefix": "https://www.baozimh.com/comic/chapter/"
                    },
                    {
                        "sufix": ".html"
                    },
					{
                        "match": "&section_slot=",
                        "text": "/"
                    },
					{
                        "match": "&amp;section_slot=",
                        "text": "/"
                    },
					
                    {
                        "match": "&amp;chapter_slot=",
                        "text": "_"
                    },
					{
                        "match": "&chapter_slot=",
                        "text": "_"
                    },
                    {
                        "match": "amp;",
                        "text": ""
                    }
					
					
                ]
            },
            "uniq": {
                "after": "?comic_id",
                "token1": "chapter_slot=",
                "token2": "\"",
                "replace": [
                    {
                        "match": "412213123",
                        "text": "Том_"
                    },
                    {
                        "match": "313123123123",
                        "text": "_Глава_"
                    }
                ]
            },
            "title": {
                "after": "?comic_id",
                "token1": ">",
                "token2": "</a>",
                "replace": [
                    {
                        "match": "Том ",
                        "text": "Том "
                    },
                    {
                        "match": " Глава ",
                        "text": " Глава "
                    },
					{
                    "match": "<.*?>",
                    "text": ""
                }
                ]
            }
        }
    },
    "chapter_complete": {
        "add_numeration": true,
		
		"headers": {
		"Accept": "*/*",
		"Referer": "https://www.baozimh.com/"
		},
		
		
       
        "add_images": {
            "start": "<div class=\"chapter-main",
            "token1": "{\"url\": \"",
            "token2": "\"",
            "end": "</body>",
			"replace": 
			[   
			{ "match": "baozicdn", "text": "baozimh" },
			{ "match": "\\\\", "text": "" }
			]
        }
    },
