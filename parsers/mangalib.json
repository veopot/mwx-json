                    {
                        "match": "\",\"eng_name\":\"",
                        "text": " | "
                    },
					{
                    "match": "\",\"model\":\"manga",
                    "text": ""
                    },
                    {
                        "clear_escape": true
                    }
                ]
            },
            "uniq": {
                "token1": "\"slug\":\"",
                "token2": "\""
