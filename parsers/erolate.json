		"name": "Veopot",
		"email": "fant-net@ya.ru"
	},

	"authorization_complete": {
		"logo": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/erolate.png",
		"registration": "https://erolate.com/register",
		"expires": 31536000000,
		"is_redirect": true,

		"values": {

			"login[login]": {
				"label": "Логин",
				"hint": "Введите свой логин с сайта Rulate!",
				"kind": "text"
			},

			"login[pass]": {
				"label": "Пароль",
				"hint": "Введите свой пароль с сайта Rulate!",
				"kind": "password"
			},
			"headers": {
				"Accept": "*/*"
			},

			"mature": {
				"forward": "cookie",
				"value": "c3a2ed4b199a1a15f5a5483504c7a75a7030dc4bi%3A1%3B"
			}
		},

		"request": {
			"method": "post",
			"url": "https://erolate.com"
		},
		"response_check": {
			"headers": {
				"Set-Cookie": "@%22login"

			}
		}
	},

	"manga_search_complete": {
		"search_link": "https://erolate.com/search/index/t/$query$/cat/0/s_lang/0/t_lang/0/sort/4/n_chapters//Book_page/$page$",
		"query_type": "GET",
		"add_manga": {
			"start": "<ul class='search-results'>",
			"next": "<a href='/book/",

			"link": {
				"skip": "<a",
				"token1": "href='/book/",
				"token2": "'>",
				"replace": {
					"prefix": "https://erolate.com/book/"
				}
			},
			"title": {
				"skip": "href='",
				"token1": "'>",
				"token2": "</"
			},
			"uniq": {
				"skip": "href='",
				"token1": "book/",
				"token2": "'>"
			},

			"summary": {
				"after": "<div id='book",
				"token1": "'>",
				"token2": "</div>",
				"replace": {
					"match": "<.*?>",
					"text": ""
				}
			},

			"cover": {
				"before": "href='",
				"token1": "src='",
				"token2": "'",
				"replace": {
					"prefix": "https://erolate.com/"
				}
			}
		}
	},

	"manga_list_complete": {
		"iterator": {
			"base_url": "https://erolate.com/search/index/cat/2/Book_page/",
			"append_nums": {
				"from": 1,
				"to": 41
			}
		},


		"add_manga": {
			"start": "</h1>",
			"end": "</h5>",
			"next": "<p class='book-tooltip'",

			"link": {
				"token1": "href='/book/",
				"token2": "'>",
				"replace": {
					"prefix": "https://erolate.com/book/"
				}
			},
			"title": {
				"skip": "href='/book/",
				"token1": "'>",
				"token2": "</"
			},
			"uniq": {
				"skip": "href='",
				"token1": "book/",
				"token2": "'>"
			}
		}

	},

	"manga_complete": {
		"headers": {
			"cookie": "mature=c3a2ed4b199a1a15f5a5483504c7a75a7030dc4bi%3A1%3B"
		},

		"title": {
			"token1": "t_title: '",
			"token2": "',"
		},
		"uniq": {
			"skip": "var Book = new CBook",
			"token1": "id:",
			"token2": ","
		},
		"summary": {
			"after": "property=\"ranobe:description\"",
			"token1": "content=\"",
			"token2": "\"",
				"replace": [
				    { "match": "&quot;", "text": "'" },
					{ "match": "<.*?>", "text": " " }
				]

		},
		

		"status": {
			"empty": {
				"start": "[<span style=",
				"end": "</span>]",
				"value": "В этом переводе ещё не создано ни одной главы."
			},
			"complete": {
				"start": "[<span style=",
				"end": "</span>]",
				"values": [">Завершён", ">Переведена"]
			},
			"ongoing": {
				"start": "[<span style=",
				"end": "</span>]",
				"values": [">В работе", ">Ожидание", ">Перерыв"]
			},
			"default": "unknown"
		},



		"add_tags": {
			"start": "class=\"book-description\">",
			"end": "class=\"swiper-wrapper\">",
			"after": "%5B0%5D=",
			"token1": "class=\"badge\">",
			"token2": "</"
		},
		"is_mature": {
			"start": "<div id=\"Info\">",
			"end": "class=\"dropdown-menu\"",
			"values": ["+18", "18+", "эротика"]
		},
		"cover": {
			"after": "<meta property=\"og:image\"",
			"token1": "content=\"",
			"token2": "\""
		},

		"content_type": {
			"default": "light_novels"
		},


		"add_chapter": {
			"start": "<tbody>",
			"end": ">Обсуждение:</",
			"next": "<td class=",
			"link": {
				"before": "title='Получить",
				"token1": "href='",
				"token2": "'"
			},
			"uniq": {
				"before": "title='Получить",
				"token1": "'t'>",
				"token2": "</a",
				"replace": [{
						"match": "<.*>",
						"text": ""
					},
					{
						"match": "Главы",
						"text": "Глава"
					},
					{
						"match": "[^a-zA-Z0-9]",
						"text": ""
					}
				]
			},

			"title": {
				"before": "title='Получить",
				"token1": "'t'>",
				"token2": "</a",
				"replace": [{
						"match": "<.*>",
						"text": ""
					},
					{
