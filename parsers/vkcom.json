				"summary" : "@^(Данная группа)",
				"chapters_count" : ">=20",
				"chapters" : [
				{
            "folder": "@martial_peak_club-1448/",
            "link": "https://vk.com/@martial_peak_club-1448",
            "title": "1448"
        },
        {
            "link": "https://vk.com/@martial_peak_club-1447",
            "title": "1447"
        }
				]
			}
		},
		{
