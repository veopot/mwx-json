	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/desu.zip",
    "file_type": "sqlite",
		
	"manga_list_complete": {
		"iterator": {
			"base_url": "https://desu.me/manga/api/?limit=30&order=popular&page=",
			"append_nums": { "from": 1, "to": 80 }
		},
		"add_manga": {
			"next": "\"id\"",
			"link":  { "after": "\"url\"", "token1": ":\"", "token2": "\",",
			"replace" : { "match" : "desu.me/manga/", "text" : "desu.me/manga/api/" }},
			"uniq": { "after": "\"name\"", "token1": ":\"", "token2": "\"," },
			"title": { "after": "\"russian\"", "token1": ":\"", "token2": "\"," }
		},	 
		"rating_auto_inc": {
			"start": "1000",
			"inc": -1
		}
	},

	"manga_complete": [
		{"condition": "response",
			"title":   { "after": "\"name\"", "token1": ":\"", "token2": "\",\"im", "replace": { "match": "\",\"russian\":\"", "text": " / " } },
			"uniq":    { "after": "\"name\"", "token1": ":\"", "token2": "\"," },
			"status":  { "complete": [ "\"status\":\"singl\"", "\"status\":\"released\"" ], "ongoing": "\"status\":\"ongoing\"", "empty": "\"status\":\"anons\"" },
			"summary": { "skip": "\"description\"", "token1": ":\"", "token2": "\"," },
			"read_dir":{ "right_to_left": "\"right-to-left\"","left_to_right": "\"left-to-right\"" },
			"cover":   { "skip": "\"original\"", "token1": ":\"", "token2": "\"" },
			"is_mature": "\"adult\":1,",
			"add_genre": { 
				"!start": "\"genres\":[",
				"!end": "],",
				"token1": "\"text\":\"",
				"token2": "\"" },
			"add_chapter": {
				"start": "\"list\":[",
				"next":  "\"id\"",
				"end": "}}}",
				"link":  { "token1": "\"id\":", "token2": ",", "replace": {"prefix" : "%%url%%chapter/" } },
				"title": { "token1": "\"vol\":", "token2": "\"date\"", "replace": [{ "match" : ",\"ch\":", "text": " - " }, { "match" : "\"title\"", "text": "" }, { "match" : ",:null,", "text": "" }, { "match" : ",:\"", "text": " - " }, { "match" : "\",", "text": "" } ] },
				"uniq":  { "token1": "\"ch\":", "token2": "\"date\"", "replace": [{ "match" : "\"title\"", "text": "" }, { "match" : ",:null,", "text": "" }, { "match" : ",:\"", "text": " - " }, { "match" : "\",", "text": "" }, { "match" : "\\\\", "text": "" }, { "match" : "\\?", "text": "" }, { "match" : "!", "text": "" }] }
			}
		},
		{"condition": "",
			"title":      { "skip": "<span class=\"rus-name\"", "token1":"\">", "token2": "</" },
			"uniq":       { "skip": "<span class=\"rus-name\"", "token1":"\">", "token2": "</" },
			"status":      { "complete": [ "Сингл", "вышло" ], "ongoing": "выходит" },
			"summary":     { "skip": "<div class=\"prgrph\"", "token1": ">", "token2": "</div>" },
			"read_dir":    { "right_to_left": "class=\"valuev\">Манга", "left_to_right": "class=\"valuev\">Манхва" },
			"cover":       { "skip": "<div class=\"c-poster\"", "token1": "src=\"", "token2": "\"" },
			"add_genre":   { 
				"!start": "<ul class=\"tagList\"",
				"!end": "</ul>",
				"token1": "href=\"manga/?genres=",
				"token2": "\"" 
			},
			"chapters_from_page": { "skip": "<meta property=\"og:url\"", "token1": "content=\"", "token2": "\"",
			"replace" : { "match" : "desu.me/manga/", "text" : "desu.me/manga/api/" }},
			"add_chapter": {
				"start": "\"list\":[",
				"next":  "\"id\"",
				"end": "user_list",
				"link":  { "token1": "\"id\":", "token2": ",", "replace": {"prefix" : "%%url%%chapter/" } },
				"title": { "token1": "\"vol\":", "token2": "\"date\"", "replace": [{ "match" : ",\"ch\":", "text": " - " }, { "match" : "\"title\"", "text": "" }, { "match" : ",:null,", "text": "" }, { "match" : ",:\"", "text": " - " }, { "match" : "\",", "text": "" } ] },
				"uniq":  { "token1": "\"ch\":", "token2": "\"date\"", "replace": [{ "match" : "\"title\"", "text": "" }, { "match" : ",:null,", "text": "" }, { "match" : ",:\"", "text": " - " }, { "match" : "\",", "text": "" }, { "match" : "\\\\", "text": "" }, { "match" : "\\?", "text": "" }, { "match" : "!", "text": "" } ] }
			}
		}
	],
	"chapter_complete": {
		"images": true,
		"add_pages": {
			"start": "\"pages\"",
			"token1": "\"img\":\"",
			"token2": "\"",
			"end": "}]"
		}
	},

