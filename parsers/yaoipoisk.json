{
   "id":5512,
   "version":1,
   
    "title":"YaoiPoisk",
   "host":"https://m2.yaoipoisk.ru",
   "public_link":"https://m2.yaoipoisk.ru/manga-list",
   "folder":"mangapoisk",
   "name":"mangapoisk",
   "language":"ru",
   "encoding":"utf-8",
   "chapters_order":false,
   "is_mature": true,
   
   "icon_link": "https://m2.yaoipoisk.ru/apple-touch-icon.png",
	"self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/yaoipoisk.json",
	"self_link2": "https://fant.my1.com/mwparser/mwx/json/yaoipoisk.json",
	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/yaoipoisk.zip",
    "file_type": "sqlite",
	
   
   "author":{
      "name":"Veopot",
      "email":"fant-net@ya.com"
   },
   "manga_list_complete":{
      "iterator":{
         "base_url":"https://m2.yaoipoisk.ru/manga?page=",
         "append_nums":{
            "from":0,
            "to":84,
            "step":1
         }
      },
      "add_manga":{
         "next":"<div class=\"manga-card group",
		 
         "title":{
            "skip":"href=\"",
            "token1":"title=\"",
            "token2":"\""
         },
         "link":{
            "token1":"href=\"",
            "token2":"\"",
			"replace": { "prefix": "https://m2.yaoipoisk.ru"}
         },
         "cover":{
		    "before":"href=\"",
            "token1":"data-src=\"https://static2.mangapoisk.me/posters",
            "token2":"\"",
			"replace": { "prefix": "https://static2.yaoipoisk.ru/posters"}
         }
      },
		"rating_auto_inc": {
			"start": "20000",
			"inc": -1
		}
   },
   
   
   "manga_search_complete": {		
        "search_link":"https://m2.yaoipoisk.ru/search?q=$query$&page=$page$",
		"query_type": "GET",
		
		 "add_manga":{
		 
         "next":"<article class=\"flex-item",
		 
         "title":{
            "skip":"href=\"",
            "token1":"class=\"card-title js-card-title\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
		 
		 "summary": { "skip":"href=\"", "token1": "<p class=\"card-text\">", "token2": "</p>",
                "replace":{"match":"<.*?>", "text":""} },
				
         "cover":{
			"skip":"href=\"",
		    "token1":"https://static2.mangapoisk.me/posters/",
            "token2":"\"",
			"replace": { "prefix": "https://static2.yaoipoisk.ru/posters/"}
         }
      }
		},	
		
		
   
   "manga_complete": { 
	   "headers": {
			"User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/88.0.3987.163 Safari/537.36",
			"Referer": "https://m2.yaoipoisk.ru"
		},
      "title":{
         "skip":"<header class=\"card-header p-0\"><h1", 
         "token1":"\">",
         "token2":"</h1>" ,
		 
		 "replace": [
				{"match": "Манга","text": ""},
				{"match": "Манхва","text": ""},
				{"match": "/","text": "|"},
				{"match":"<.*?>","text":""}

				]},
      
      "uniq":{
         "token1":"\"url\":\"https:\/\/m2.yaoipoisk.ru\/manga\/",
         "token2":"\","
      },
	  
	  
      "cover":{
		"token1":"src=\"https://static2.mangapoisk.me/posters/",
        "token2":"\"",
		"replace": {
				"prefix": "https://static2.yaoipoisk.ru/posters/"
				}
		},
	  
	  "rating":{
         "token1":"\"ratingCount\":",
         "token2":"</"
      },
      
	  "status": { 
		  	"complete": { 
			 "start": ">Статус:<",
             "end": ">Жанр:<",
			   "values": [ "Завершена", "Переведена", "Сингл"] },
			
            "ongoing": { 
			"start": ">Статус:<",
             "end": ">Жанр:<",
                "value": "Выпускается" }
			 },
			 
			 
      "summary":{
         "skip":"class=\"manga-description",
         "token1":"\">",
         "token2":"</div>",
         "replace":{
            "match":"<.*?>",
            "text":""
         }
      },
      "add_tags":{
         "start":">Жанр:<",
         "end":"\">Год:</b>",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            "эротика",
            "сэйнэн",
            "юри",
			"этти",
            "яой"
         ]
      },
	  
	
	  "chapters_from_page": { 
		
		"token1": "&quot;url&quot;:&quot;/manga/",
		"token2": "&quot", 
		"replace": [
		        { "prefix" : "https://m2.yaoipoisk.ru/manga/" },
		        { "sufix" : "/chaptersList" },
				{ "match": "\\\\", "text": "" }
				],
				
			"next": {
				"before": "rel=\"next\"",
				"token1": "href=\"",
				"token2": "\"",
				
				"replace": [
				    {"prefix": "https://m2.yaoipoisk.ru" },
					{ "match": "&amp;", "text": "&" }
				]
				
				
				
			}	
		
		},
	  
	  
	  
		
		"add_chapter":{
        "start": "</head>",
		 "end": "class=\"chapters-infinite-pagination\"",
        
		 
         "next": "chapter-item",
		 
		                 
         "link":{
			
            "token1":"href=\"",
            "token2":"\"",
			"replace": {"prefix" : "https://m2.yaoipoisk.ru"}
         },
		 
		"uniq":{
            "skip":"href=\"",
            "token1":"/chapter/",
            "token2":"\""
        },
		 
        "title":{
            "skip":"href=\"",
            "token1":"class=\"chapter-title\">",
            "token2":"</a>",
			"replace": { "match": "</span>",	"text": ""}
        }
      }
	  
	  
   },
   
   "chapter_complete":{
    "add_numeration": true,
        "headers": {
			"Accept": "*/*",
			"Referer": "https://m2.yaoipoisk.ru/"
		},
     
      "images":true,
"add_pages":

	{
		"start": "class=\"pt-4 mx-auto outline-0 cursor-auto w-full\"",
		"before": "id=\"",
		"token1": "src=\"https://static2.mangapoisk.me/pages/",
		"token2": "\"",
		"end": ">Комментарии",
		"replace":
		[{
				"prefix": "https://static2.yaoipoisk.ru/pages/"
			}, {
				"match": "data-small-src=\"(.*?)\"",
				"text": ""
			},
			{
				"match": "src=\"data:image/(.*?)\"",
				"text": ""
			}
		]
	}

	},
	
	
   "test":[
      {
         "proc":"manga_list_complete",
         "assert":{
            "mangas":[
               {
                  "title":"Ванпанчмен",
                  "link":"https://m2.yaoipoisk.ru/manga/vanpanchmen",
                  "uniq":"vanpanchmen"
               }
            ],
            "mangas_count":">=14"
         }
      },
      {
         "proc":"manga_complete",
