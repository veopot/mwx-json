    "file_type": "sqlite",
    "author": {
        "name": "Veopot",
        "email": "fant-net@ya.com"
    },
    "manga_list_complete": {
        "iterator": {
            "base_url": "https://m1.yaoi-poisk.ru/manga?page=",
            "append_nums": {
                "from": 0,
                "to": 143,
                "step": 1
            }
        },
        "add_manga": {
            "next": "<div class=\"manga-card group",
            "title": {
                "skip": "href=\"",
                "token1": "title=\"",
                "token2": "\""
            },
            "link": {
                "token1": "href=\"",
                "token2": "\"",
                "replace": {
                    "prefix": "https://m1.yaoi-poisk.ru"
                }
            },
            "cover": {
                "before": "href=\"",
                "token1": "data-src=\"https://static2.mangapoisk.net/posters",
                "token2": "\"",
                "replace": {
                    "prefix": "https://static2.mangapoisk.net/posters"
                }
            }
        },
        "rating_auto_inc": {
            "start": "20000",
            "inc": -1
        }
    },
    "manga_search_complete": {
        "search_link": "https://m1.yaoi-poisk.ru/search?q=$query$&page=$page$",
        "query_type": "GET",
        "add_manga": {
            "next": "<article class=\"flex-item",
            "title": {
                "skip": "href=\"",
                "token1": "class=\"card-title js-card-title\">",
                "token2": "</"
            },
            "link": {
                "token1": "href=\"",
                "token2": "\""
            },
            "summary": {
                "skip": "href=\"",
                "token1": "<p class=\"card-text\">",
                "token2": "</p>",
                "replace": {
                    "match": "<.*?>",
                    "text": ""
                }
            },
            "cover": {
                "skip": "href=\"",
                "token1": "https://static2.mangapoisk.net/posters/",
                "token2": "\"",
                "replace": {
                    "prefix": "https://static2.mangapoisk.net/posters/"
                }
            }
        }
    },
    "manga_complete": {
        "headers": {
            "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/88.0.3987.163 Safari/537.36",
            "Referer": "https://m1.yaoi-poisk.ru"
        },
        "title": {
            "skip": "<header class=\"card-header p-0\"><h1",
            "token1": "\">",
            "token2": "</h1>",
            "replace": [
                {
                    "match": "Манга",
                    "text": ""
                },
                {
                    "match": "Манхва",
                    "text": ""
                },
                {
                    "match": "/",
                    "text": "|"
                },
                {
                    "match": "<.*?>",
                    "text": ""
                }
            ]
        },
        "uniq": {
            "token1": "\"url\":\"https:\/\/m1.yaoi-poisk.ru\/manga\/",
            "token2": "\","
        },
        "cover": {
            "token1": "src=\"https://static2.mangapoisk.net/posters/",
            "token2": "\"",
            "replace": {
                "prefix": "https://static2.mangapoisk.net/posters/"
            }
        },
        "rating": {
            "token1": "\"ratingCount\":",
            "token2": "</"
        },
        "status": {
            "complete": {
                "start": ">Статус:<",
                "end": ">Жанр:<",
                "values": [
                    "Завершена",
                    "Переведена",
                    "Сингл"
                ]
            },
            "ongoing": {
                "start": ">Статус:<",
                "end": ">Жанр:<",
                "value": "Выпускается"
            }
        },
        "summary": {
            "skip": "class=\"manga-description",
            "token1": "\">",
            "token2": "</div>",
            "replace": {
                "match": "<.*?>",
                "text": ""
            }
        },
        "add_tags": {
            "start": ">Жанр:<",
            "end": "\">Год:</b>",
            "token1": "\">",
            "token2": "</"
        },
        "is_mature": {
            "values": [
                "эротика",
                "сэйнэн",
                "юри",
                "этти",
                "яой"
            ]
        },
        "chapters_from_page": {
            "token1": "&quot;url&quot;:&quot;/manga/",
            "token2": "&quot",
            "replace": [
                {  "prefix": "https://m1.yaoi-poisk.ru/manga/" },
                {  "sufix": "/chaptersList" },
                {  "match": "\\\\",  "text": "" }
                    ],
					
            "next": {
                "before": "rel=\"next\"",
				
                "token1": "href=\"",
				
                "token2": "\"",
                "replace": [
                    {
                        "prefix": "https://m1.yaoi-poisk.ru"
                    },
                    {
                        "match": "&amp;",
                        "text": "&"
                    }
                ]
            }
        },
        "add_chapter": {
            "start": "</head>",
            "end": "class=\"chapters-infinite-pagination\"",
            "next": "chapter-item",
            "link": {
                "token1": "href=\"",
                "token2": "\"",
                "replace": {
                    "prefix": "https://m1.yaoi-poisk.ru"
                }
            },
            "uniq": {
                "skip": "href=\"",
                "token1": "/chapter/",
                "token2": "\""
            },
            "title": {
                "skip": "href=\"",
                "token1": "class=\"chapter-title\">",
                "token2": "</a>",
                "replace": {
                    "match": "</span>",
                    "text": ""
                }
            }
        }
    },
    "chapter_complete": {
        "add_numeration": true,
        "headers": {
            "Accept": "*/*",
            "Referer": "https://m1.yaoi-poisk.ru/"
        },
        "images": true,
        "add_pages": {
            "start": "class=\"pt-4 mx-auto outline-0 cursor-auto w-full\"",
            "before": "id=\"p",
            "token1": "src=\"https://static2.mangapoisk.live/pages/",
            "token2": "\"",
            "end": ">Комментарии",
            "replace": [
                {
                    "prefix": "https://static2.mangapoisk.live/pages/"
                },
                {
                    "match": "data-small-src=\"(.*?)\"",
                    "text": ""
                },
                {
                    "match": "src=\"data:image/(.*?)\"",
                    "text": ""
                }
            ]
        }
    },
