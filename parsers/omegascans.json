                "chapters_count": ">=2",
                "chapters": [
                    {
            "folder": "Chapter 1\\",
            "link": "https://omegascans.org/series/god-bless-you/chapter-1/",
            "title": "Chapter 1"
        },
        {
            "link": "https://omegascans.org/series/god-bless-you/chapter-2/",
            "title": "Chapter 2"
        }
                ]
            }
        },
        {
            "proc": "chapter_complete",
            "link": "https://omegascans.org/series/god-bless-you/chapter-1",
            "assert": {
