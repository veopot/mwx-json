    "manga_search_complete": {
        "headers": {
            "accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
            "accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
            "referer": "https://www.mgeko.cc/"
        },
        "search_link": "https://www.mgeko.cc/search/?search=$query$",
        "query_type": "GET",
        "add_manga": {
            "start": "<header id=\"Result\">",
            "end": "</main>",
            "next": "class=\"novel-item\">",
            "link": {
                "token1": "href=\"",
                "token2": "\"",
                "replace": {
                    "prefix": "https://www.mgeko.cc"
                }
            },
            "title": {
                "token1": "title=\"",
                "token2": "\""
            },
            "uniq": {
                "token1": "href=\"/manga/",
                "token2": "/\""
            },
            "summary": {
                "token1": "<div class=\"summary truncate-fade\" title=\"",
                "token2": "\"",
                "replace": {
                    "match": "<.*?>",
                    "text": ""
                }
            },
            "cover": {
                "token1": "data-src=\"",
                "token2": "\"",
                "replace": {
                    "prefix": "https://www.mgeko.cc"
                }
            }
        }
    },
    "manga_list_complete": {
        "headers": {
            "accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
            "accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
            "referer": "https://www.mgeko.cc/"
        },
        "iterator": {
            "base_url": "https://www.mgeko.cc/browse-comics/?genre_included=&genre_excluded=&tags_include=&tags_exclude=&minchaps=0&filter=Ratings&results=",
            "append_nums": {
                "from": 0,
                "to": 249,
                "step": 1,
                "sufix": "&filter=views"
            }
        },
        "add_manga": {
            "start": "<header id=\"Result\">",
            "end": "</main>",
            "next": "class=\"novel-item\">",
            "link": {
                "token1": "href=\"",
                "token2": "\"",
                "replace": {
                    "prefix": "https://www.mgeko.cc"
                }
            },
            "title": {
                "token1": "title=\"",
                "token2": "\""
            },
            "uniq": {
                "token1": "href=\"/manga/",
                "token2": "/\""
            }
        },
        "rating_auto_inc": {
            "start": "30000",
            "inc": -1
        }
    },
    "manga_complete": {
        "headers": {
		"accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
        "accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
		"referer": "https://www.mgeko.cc/"
        },
        "title": {
            "skip": "<figure class=\"cover\">",
            "token1": "alt=\"",
            "token2": "\""
        },
        "uniq": {
            "after": "property=\"og:url\"",
            "token1": "content=\"https://www.mgeko.cc/manga/",
            "token2": "/\""
        },
        "author": {
            "token1": "<span itemprop=\"author\">",
            "token2": "</"
        },
        "summary": {
            "after": "<p class=\"description",
            "token1": ">",
            "token2": "</p>",
            "replace": [
                {
                    "match": "\n",
                    "text": " "
                },
                {
                    "match": "\r",
                    "text": " "
                },
                {
                    "match": "<.*?>",
                    "text": ""
                }
            ]
        },
        "status": {
            "single": ">Сингл</",
            "complete": "<strong class=\"completed\">",
            "ongoing": "<strong class=\"ongoing\">"
        },
        "is_mature": {
            "start": ">Categories<",
            "end": "</ul>",
            "values": [
                "Mature",
                "Seinen",
                ">эротика<"
            ]
        },
        "cover": {
            "after": "<figure class=\"cover\">",
            "token1": "data-src=\"",
            "token2": "\""
        },
        "add_tag": {
            "after": "href=\"/browse-comics/?genre",
            "token1": "\">",
            "token2": "</"
        },
		
		
		
		
        "add_chapter": {
            "headers": {
                "accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
                "accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
                "referer": "https://www.mgeko.cc/"
            },
            "start": "class=\"chapter-list\"",
            "end": "<footer translate",
            "next": "<li data-chapterno",
            "link": {
                "token1": "href=\"",
                "token2": "\""
            },
            "title": {
                "skip": "href=\"",
                "token1": "class=\"chapter-title\">",
                "token2": "</",
                "replace": [
                    {
                        "match": "-",
                        "text": " "
                    },
                    {
                        "prefix": "Chapters "
                    }
                ]
            },
            "uniq": {
               "skip": "href=\"",
               "token1": "class=\"chapter-title\">",
                "token2": "</"
            }
        }
    },
    "chapter_complete": {
        "headers": {
