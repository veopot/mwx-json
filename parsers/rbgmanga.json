{
    "id": 40155,
    "version": 99,
    "title": "Re:Manga",
    "host": "https://api.remanga.org",
    "public_link": "https://remanga.org/manga",
    "folder": "rbgmanga",
    "name": "rbgmanga",
    "chapters_order": true,
    "language": "ru",
    "encoding": "utf-8",

    "icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/rbgmanga.png",
    "self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/rbgmanga.json",
    "self_link2": "https://fant.my1.ru/mwparser/mwx/json/rbgmanga.json",
    "catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/rbgmanga.zip",
    "file_type": "sqlite",
    "author": {
        "name": "Veopot",
        "email": "fant-net@ya.ru"
    },
    
    "manga_search_complete": {
       "headers": {
			"cookie": "token=qFQdhQwOVTj7IQtaSsTswac7dtIGFr",
            
            "Referer": "https://remanga.org/",
	 "User-Agent": "Mozilla/5.0 (Linux; Android 10; K) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/124.0.0.0 Safari/537.36"
			},
        "search_link": "https://api.remanga.org/api/search/?count=10&page=$page$&query=$query$",
        "query_type": "GET",
        "add_manga": {
            "start": "content\"",
            "end": "props\"",
            "next": "bookmark_type\"",
            "link": {
                "before": "bookmark_type\"",
                "token1": "dir\":\"",
                "token2": "\",",
                "replace": [
                    {
                        "prefix": "https://api.remanga.org/api/titles/"
                    },
                    {
                        "sufix": "/"
                    }
                ]
            },
            "cover": {
                "skip": "bookmark_type\"",
                "token1": "mid\":\"",
                "token2": "\"",
                "replace": {
                    "prefix": "https://remanga.org"
                }
            },
            "title": {
                "before": "bookmark_type\"",
                "token1": "main_name\":\"",
                "token2": "\",\"translate_status",
                "replace": {
                    "match": "\",\"secondary_name\":\"",
                    "text": " | "
                }
            },
            "summary": {
                "token1": "\"issue_",
                "token2": "\"bookmark_type\"",
                "replace": [
                    {
                        "match": "year\":",
                        "text": "Год выпуска "
                    },
					{
                        "match": "\",",
                        "text": ","
                    },
                    {
                        "match": "\"avg_rating\":\"",
                        "text": " Рейтинг "
                    },
                    {
                        "match": "\"is_yaoi\":",
                        "text": "яой:"
                    },
					{
                        "match": "\"is_erotic\":",
                        "text": "эротика:"
                    },
					{
                        "match": "false",
                        "text": "нет"
                    },
					{
                        "match": "true",
                        "text": "да"
                    },
					{
                        "match": "\"admin_rating\":\"(.*?)\",",
                        "text": ""
                    },
					
					
					
					{
                        "match": "\"type\":(.*?),",
                        "text": ""
                    },
					
					
					
					
                    {
                        "match": "\"count_chapters\":",
                        "text": " Доступно Глав "
                    }
                ]
            },
            "uniq": {
                "before": "bookmark_type\"",
                "token1": "en_name\":\"",
                "token2": "\""
            }
        }
    },
    "manga_list_complete": {
        "headers": {
			"cookie": "token=qFQdhQwOVTj7IQtaSsTswac7dtIGFr",
           
            "Referer": "https://remanga.org/",
			"User-Agent": "Mozilla/5.0 (Linux; Android 10; K) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/124.0.0.0 Safari/537.36"
			},
        "iterator": {
            "base_url": "https://api.remanga.org/api/search/catalog/?ordering=-rating&salt=&types=0&types=1&types=2&types=3&types=4&types=5&count=30&page=",
            "append_nums": {
                "from": 1,
                "to": 787
            }
        },
        "add_manga": {
            "start": "content",
            "end": "props\"",
            "next": "bookmark_type\"",
            "link": {
                "before": "bookmark_type\"",
                "token1": "dir\":\"",
                "token2": "\",",
                "replace": [
                    {
                        "prefix": "https://api.remanga.org/api/titles/"
                    },
                    {
                        "sufix": "/"
                    }
                ]
            },
            "cover": {
                "before": "bookmark_type\"",
                "token1": "cover_mid\":\"",
                "token2": "\"",
                "replace": {
                    "prefix": "https://api.remanga.org/media/"
                }
            },

            "title": {
                "before": "bookmark_type\"",
                "token1": "secondary_name\":\"",
                "token2": "\",\"dir",
                "replace": {
                    "match": "\",\"main_name\":\"",
                    "text": " | "
                }
            },


            
            "uniq": {
                "before": "bookmark_type\"",
                "token1": "en_name\":\"",
                "token2": "\""
            }
        },
	    "link_corrector": [
            {
                "match": "remanga.org/manga/",
                "text": "api.remanga.org/api/titles/"
            }
        ],
        "rating_auto_inc": {
            "start": "40000",
            "inc": -1
        }
    },
    "manga_complete": {
		
        "headers": {
			"cookie": "token=qFQdhQwOVTj7IQtaSsTswac7dtIGFr",
            
            "Referer": "https://remanga.org/",
            "User-Agent": "Mozilla/5.0 (Linux; Android 10; K) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/124.0.0.0 Safari/537.36" 
			},
        
        "title": {
            "token1": "secondary_name\":",
            "token2": "\"dir\":\"",
            "replace": [
                {
                    "match": "\"(.*?)\",\"(.*?)\":\"(.*?)\",",
                    "text": "$3 | $1"
                },
                {
                    "match": "\"another_name\":\"(.*?)\",",
                    "text": ""
                },
                {
                    "match": "\"",
                    "text": "'"
                },
                {
                    "match": "\\\\",
                    "text": ""
                }
            ]
        },
        "additional_title": {
            "token1": "secondary_name\":\"",
            "token2": "\","
        },
        "uniq": {
            "token1": "\"dir\":\"",
            "token2": "\","
        },
        "cover": {
            "token1": "high\":\"",
            "token2": "\"",
            "replace": [
                {
                    "prefix": "https://remanga.org/media/"
                },
                {
                    "match": "u002F",
                    "text": "/"
                }
            ]
        },
        "summary": {
            "token1": "description\":\"",
            "token2": "\",",
            "replace": [
                {
                    "match": "u003Cp",
                    "text": ""
                },
                {
                    "match": "u003E",
                    "text": ""
                },
                {
                    "match": "u003C",
                    "text": ""
                },
                {
                    "match": "<.*?>",
                    "text": ""
                },
                {
                    "match": "u002Fp",
                    "text": ""
                },
                {
                    "match": "&quot;",
                    "text": "'"
                }
            ]
        },
        "need_auth": "авторизироваться",
        "status": {
            "complete": {
                "start": "\"status\"",
                "end": "\"props",
                "values": ["Закончен", "Переведена", "Сингл"]
            },
            "ongoing": {
                "start": "\"status\"",
                "end": "\"props",
                "value": "Продолжается"
            }
        },
        "chapters": {
            "start": "content",
            "end": "similarManga",
            "token1": "total_chapters\":",
            "token2": ","
        },
        "add_genres": {
            "start": "genres\":",
            "end": "],",
            "token1": "name\":\"",
            "token2": "\""
        },
        "add_tags": {
            "start": "categories\":",
            "end": "],",
            "token1": "name\":\"",
            "token2": "\""
        },
        "is_mature": {
            "values": ["Сэйнэн", "Эротика", "Юри", "Яой"]
        },
        "chapters_from_page": {
            "after": "branches\"",
            "token1": "id\":",
            "token2": ",",
            "replace": [
                {
                    "prefix": "https://api.remanga.org/api/titles/chapters/?branch_id="
                },
                {
                    "sufix": "&count=100&ordering=-index&user_data=0&page=%%iterator_num%%"
                }
            ],
            "iterator": {
                "base_url": "",
                "append_nums": {
                    "from": 1
                }
            }
        },
        "add_chapter": {
			"headers": {
			"cookie": "token=qFQdhQwOVTj7IQtaSsTswac7dtIGFr",
           
            "Referer": "%%url%%",
			"User-Agent": "Mozilla/5.0 (Linux; Android 10; K) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/124.0.0.0 Safari/537.36"
			},
			
            "start": "content\"",
            "end": "props",
            "next": "\"is_paid\":false",
			
            "link": {
                "before": "\"is_paid\":false",
                "token1": "id\":",
                "token2": ",",
                "replace": [
                    {
                        "prefix": "https://api.remanga.org/api/titles/chapters/"
                    },
                    {
                        "sufix": "/"
                    }
                ]
            },
            "uniq": {
                "before": "\"is_paid\":false",
                "token1": ",\"to",
                "token2": "\",\"name\"",
                "replace": [
                    {
                        "match": "me\":",
                        "text": "Том_"
                    },
                    {
                        "match": ",\"chapter\":\"",
                        "text": "_Глава_"
                    }
                ]
            },
            "title": {
                "before": "\"is_paid\":false",
                "token1": ",\"t",
                "token2":  "\",\"is",                   
                "replace": [
                    {
                        "match": "ome\":",
                        "text": "Том "
                    },
					{
                        "match": "\",\"name\":\"",
                        "text": " "
                    },
                    {
                        "match": ",\"chapter\":\"",
                        "text": " Глава "
                    }
                ]
            }
        }
    },
    "chapter_complete": {
        "add_numeration": true,
        "headers": {
			"cookie": "token=qFQdhQwOVTj7IQtaSsTswac7dtIGFr",
            
            "Referer": "https://remanga.org/",
			"User-Agent": "Mozilla/5.0 (Linux; Android 10; K) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/124.0.0.0 Safari/537.36"
			},
        "add_images": {
            "start": "content",
            "token1": "link\":\"",
            "token2": "\"",
            "end": "props"
        }
    },
