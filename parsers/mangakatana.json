{
   "id":40226,
   "version":7,
   "title":"MangaKatana",
   "host":"https://mangakatana.com",
   "public_link":"https://mangakatana.com/manga-list",
   "folder":"mangakatana",
   "name":"mangakatana",
   "language":"en",
   "encoding":"utf-8",
   "chapters_order":false,
   
   "icon_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/img/mangakatana.png",
	"self_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/parsers/mangakatana.json",
	"self_link2": "https://fant.my1.ru/mwparser/mwx/json/mangakatana.json",
	"catalog_link": "https://raw.githubusercontent.com/veopot/mwx-json/main/catalogs/mangakatana.zip",
    "file_type": "sqlite",

    "author":{
      "name":"Veopot",
      "email":"fant-net@ya.ru"
   },
   
   
   
   "manga_list_complete":{
	   "headers": {
		"Referer": "https://mangakatana.com/",
"Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9",
            
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		
		
      "iterator":{
         "base_url":"https://mangakatana.com/manga/page/",
         "append_nums":{
            "from":1,
            "to":1290,
            "step":1,
			"sufix": "?filter=1&include_mode=and&order=numc"
         }
      },
      "add_manga":{
         "next":"class=\"media\">",
         "title":{
            "skip":"class=\"title\">",
            "token1":"\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
         "cover":{
		    
            "token1":"src=\"",
            "token2":"\""
         }
      },
		"rating_auto_inc": {
			"start": "30000",
			"inc": -1
		}
   },
   
   
   "manga_search_complete": {
	   "headers": {
		"Referer": "https://mangakatana.com/",
"Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9",
            
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		
		
 "search_link":"https://mangakatana.com/page/$page$?search=$query$&search_by=book_name",

		"query_type": "GET",
		 "add_manga":{
         "next":"class=\"media\">",
         "title":{
            "skip":"class=\"title\">",
            "token1":"\">",
            "token2":"</"
         },
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
		  
		"summary": { "after": "<div class=\"summary",  "token1": "\">", "token2": "</div>",
                "replace":{"match":"<.*?>", "text":""} },
				
				
         "cover":{
		    
            "token1":"src=\"",
            "token2":"\""
         }
      }
		},	
		
		
   
   "manga_complete":{
	   "headers": {
		"Referer": "https://mangakatana.com/",
"Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9",
            
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
		
	  
	   
	   
      "title":{ 
           "after":"property=\"og:title\"",
             "token1":"content=\"",
             "token2":"\""},
      
      "uniq":{ 
           "after":"property=\"og:title\"",
             "token1":"content=\"",
             "token2":"\""},
	  
	  
      "cover":{ 
           "after":"property=\"og:image\"",
             "token1":"content=\"",
             "token2":"\""},
      
	  "status": { 
	  "single": "Сингл<",
	  "complete": "class=\"d-cell-small value status completed\">Completed</",
	  "ongoing": "class=\"d-cell-small value status ongoing\">Ongoing</",
	  "empty": "Нет глав<" },
			 
      "summary":{ 
           "after":"property=\"og:description\"",
             "token1":"content=\"",
             "token2":"\""},
	  
	  "author": { "after": "href=\"https://mangakatana.com/author/", "token1":">", "token2": "</" },	 
			 
		 
      "add_genres":{
         "start":">Genres:</",
         "end":">Status:</",
         "token1":"\">",
         "token2":"</"
      },
      "is_mature":{
         "values":[
            ">Adult<",
            ">Sexual violence<",
            ">Seinen<",
			">Yuri<",
            ">Yaoi<"
         ]
      },
	  
	  
		
		"add_chapter":{
		
         "start":"class=\"aff_items uk-overflow-container\">",
         "end":"id=\"hot_book\">",
		 
         "next":"class=\"chapter\">",
		 
         "link":{
            "token1":"href=\"",
            "token2":"\""
         },
		 "uniq":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</a>"
         },
         "title":{
            "skip":"href=\"",
            "token1":"\">",
            "token2":"</a>"
         }
      
   }
   },
   
   
   "chapter_complete":{
    "add_numeration": true,
        "headers": {
			"Accept": "*/*",
			"Referer": "https://mangakatana.com/",
			"Accept-Language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7"
		},
     
      "images":true,
      "add_pages":{
         "start":"var ytaw",
         "token1":"=['",
         "token2":"',]",
		 "split" : "','",
         "end":"id=\"copyright\">"
      }
   },
